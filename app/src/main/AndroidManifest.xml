<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.wnw.lovebaby">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".view.activity.MainActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".view.activity.SearchGoodsActivity"/>
        <activity android:name=".view.activity.SearchResultActivity"/>
        <activity android:name=".view.activity.OrderConfirmationActivity"/>
        <activity android:name=".view.activity.AddressListActivity"/>
        <activity android:name=".view.activity.AddReceAddressActivity"/>
        <activity android:name=".view.activity.EditReceAddressActivity"/>
        <activity android:name=".view.activity.TodayNewGoodsActivity"/>
        <activity android:name=".view.activity.InviteOpenShopActivity"/>
    </application>

</manifest>